{
  "public": "./dist",
  "headers": [
    {
      "source" : "**/*.@(js|css|jpg|jpeg|gif|png)",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "max-age=31536000"
      }]
    },
    {
      "source": "**/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, private"
        }
      ]
    }
  ]
}
